{
    "rules": {
        "no-console":0,
        "strict":[
            2,
            "never"
        ],
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "esnext": true,
    "ecmaFeatures": {
        "jsx": true
    },
    "plugins": [
        "react"
    ]
}